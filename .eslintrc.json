{
  "extends": ["next/core-web-vitals", "plugin:react/recommended", "airbnb", "airbnb-typescript"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "object-curly-newline": "off",
    "max-len": "off",
    "comma-dangle": 0,
    "react/jsx-wrap-multilines": "off"
  },
  "ignorePatterns": ["components/ui/*", "*.mjs", "lib/*"],
  "parserOptions": { "project": "./tsconfig.json" }
}
